      </setting>
      <setting name="PscpHomePrefix" serializeAs="String">
        <value>/home/</value>
      </setting>
      <setting name="AutoUpdateCheck" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="PscpRootHomePrefix" serializeAs="String">
