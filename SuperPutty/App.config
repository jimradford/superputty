      </setting>
      <setting name="MinttyExe" serializeAs="String">
        <value/>
      </setting>
      <setting name="VNCExe" serializeAs="String">
        <value/>
      </setting>
      <setting name="RDPExe" serializeAs="String">
        <value/>
